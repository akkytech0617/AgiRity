{
  "name": "agirity",
  "version": "0.1.0",
  "description": "Workspace management tool that automatically launches multiple applications for project work",
  "main": "dist-electron/main/index.js",
  "author": "AgiRity Team",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build && electron-builder",
    "preview": "vite preview",
    "test": "vitest run --no-color",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "lint": "eslint --no-color .",
    "lint:fix": "eslint . --fix --no-color",
    "format": "prettier --write .",
    "format:check": "prettier --check --no-color .",
    "type-check": "tsc --pretty false -p tsconfig.json --noEmit && tsc --pretty false -p tsconfig.node.json --noEmit",
    "package": "electron-builder",
    "release": "npm run build && git tag -a v$npm_package_version -m \"v$npm_package_version\" && git push origin v$npm_package_version",
    "prepare": "lefthook install"
  },
  "dependencies": {
    "clsx": "^2.1.1",
    "electron-updater": "^6.1.7",
    "js-yaml": "^4.1.0",
    "lucide-react": "^0.344.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwind-merge": "^2.6.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@playwright/test": "^1.41.2",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20.11.24",
    "@types/react": "^18.2.61",
    "@types/react-dom": "^18.2.19",
    "@typescript-eslint/eslint-plugin": "^8.48.0",
    "@typescript-eslint/parser": "^8.48.0",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^4.0.15",
    "autoprefixer": "^10.4.18",
    "concurrently": "^8.2.2",
    "electron": "^39.2.7",
    "electron-builder": "^24.13.3",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "eslint-plugin-security": "^3.0.1",
    "globals": "^16.5.0",
    "jsdom": "^27.3.0",
    "lefthook": "^2.0.12",
    "postcss": "^8.4.35",
    "prettier": "3.7.4",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.48.0",
    "vite": "^5.1.4",
    "vite-plugin-electron": "^0.28.2",
    "vite-plugin-electron-renderer": "^0.14.5",
    "vitest": "^4.0.15"
  }
}
